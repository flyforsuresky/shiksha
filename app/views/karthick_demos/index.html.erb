<p id="notice"><%= notice %></p>

<h1>Karthick Demos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Gender</th>
      <th>Skills</th>
      <th>Interests</th>
      <th>Hobbies</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @karthick_demos.each do |karthick_demo| %>
      <tr>
        <td><%= karthick_demo.name %></td>
        <td><%= karthick_demo.age %></td>
        <td><%= karthick_demo.gender %></td>
        <td><%= karthick_demo.skills %></td>
        <td><%= karthick_demo.interests %></td>
        <td><%= karthick_demo.hobbies %></td>
        <td><%= link_to 'Show', karthick_demo %></td>
        <td><%= link_to 'Edit', edit_karthick_demo_path(karthick_demo) %></td>
        <td><%= link_to 'Destroy', karthick_demo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Karthick Demo', new_karthick_demo_path %>
