<p id="notice"><%= notice %></p>

<h1>Custom Forms Tamil Forms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Dob</th>
      <th>Place</th>
      <th>District</th>
      <th>Pincode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @custom_forms_tamil_forms.each do |custom_forms_tamil_form| %>
      <tr>
        <td><%= custom_forms_tamil_form.name %></td>
        <td><%= custom_forms_tamil_form.email %></td>
        <td><%= custom_forms_tamil_form.dob %></td>
        <td><%= custom_forms_tamil_form.place %></td>
        <td><%= custom_forms_tamil_form.district %></td>
        <td><%= custom_forms_tamil_form.pincode %></td>
        <td><%= link_to 'Show', custom_forms_tamil_form %></td>
        <td><%= link_to 'Edit', edit_custom_forms_tamil_form_path(custom_forms_tamil_form) %></td>
        <td><%= link_to 'Destroy', custom_forms_tamil_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Custom Forms Tamil Form', new_custom_forms_tamil_form_path %>
