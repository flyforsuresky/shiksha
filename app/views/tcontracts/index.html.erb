<p id="notice"><%= notice %></p>

<h1>Tcontracts</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>Teacher</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tcontracts.each do |tcontract| %>
      <tr>
        <td><%= tcontract.subject_id %></td>
        <td><%= tcontract.teacher_id %></td>
        <td><%= link_to 'Show', tcontract %></td>
        <td><%= link_to 'Edit', edit_tcontract_path(tcontract) %></td>
        <td><%= link_to 'Destroy', tcontract, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tcontract', new_tcontract_path %>
